<% layout(layoutPath) -%>
<h1>Company Settings</h1>
<form action="/cregister" method="post">
  <fieldset>
    <label>Company Name:</label>
    <input type="text" name="name" value="<%= comp.name || "" %>"/>
    <label>Website:</label>
    <input type="url" name="url" value="<%= comp.url || "" %>"/>
    <label>Public Email:</label>
    <input type="email" name="publicemail" value="<%= comp.publicemail || "" %>"/>   
    <label>Phone:</label>
    <input type="text" name="telephone" value="<%= comp.telephone || "" %>"/>
    <label>Address:</label>
    <input type="text" name="address" value="<%= comp.address || "" %>"/>
    <label>City:</label>
    <input type="text" name="city" value="<%= comp.city || "" %>"/>
    <label>State:</label>
    <input type="text" name="state" value="<%= comp.state || "" %>"/>   
    <label>Zipcode:</label>
    <input type="text" name="zipcode" value="<%= comp.zipcode || "" %>"/>
    <button class="btn">Create</button>
  </fieldset>
</form>
