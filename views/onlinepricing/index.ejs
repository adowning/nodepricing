</div>




<div class="container-fluid" id="contactform">
  <div class="row-fluid">
    <div class="span2">
    </div>
    <div class="span8"><img src="/img/acc.gif">
    </div>
    <div class="span2">
    </div>
  </div>
  <div class="row-fluid">
    <div class="span2">
    </div>
    <div class="span8">
      <div class="row-fluid">
            <div class="span1">
    </div>
        <div class="span12">
      </button>
      <h3>
        Enter Contact Information
      </h3>
  
    <div>
      <div id="contact-formx" class="clearfix">
<form action="/createonlineprice/<%= pricing.key %>" method="post">
          <div class="row">
            <div class="span5">
              <label>
                Name
              </label>
              <input name="name" value="asdf" id="name" type="text" class="span5" placeholder="John Galt" required>
              <label>
                Email Address
              </label>
              <input input type="email" value="asdf@asdf.com" type="text" class="span5" name="email" id="email" placeholder="jg@email.com" required>
              
              <label>
                Telephone
              </label>
              <input type='telephone' value='9999999999' pattern="^\D?(\d{3})\D?\D?(\d{3})\D?(\d{4})$"    name="telephone" id="telephone"  class="span5" placeholder="903 555 1234" required>
              <label>
                Address
              </label>
              <input input type="address" value='123 asdf' name="address" id="address" type="text" class="span5" placeholder="4242 Easy St." required>
     
    </div>
    <div class="span7">
      <label>
        Special Instructions for your technician:
      </label>
      <textarea placeholder="Any special instructions you would like to pass on to us or the technician goes here." name="instructions" id="instructions" class="input-xlarge span7" rows="10">
      </textarea>
    </div>
    <input type="hidden" id="city" name="city"  required/>
    <input type="hidden" id="state" name="state"  required/>
    <input type="hidden" id="scheduledate" value='laskdjf' name="scheduledate" required/>
    <input type="hidden" id="scheduletime" value='asdf' name="scheduletime"  required/>
    <input type="hidden" id="zipcode" name="zipcode"  required/>
    <input type="hidden" id="services" name="services"  required/>
        <input type="hidden" id="services_totals" name="services_totals"  required/>
    <div class="row"></div>
    <div class="span6"></div><div class="span3" name="formbuttons" id="formbuttons">
        <button type="button" value="Cancel" class="btn" aria-hidden="true" onclick="$('#contactform').hide(); $('#scheduleform').show(); return false;">
      <i class="icon-arrow-left ">
      </i>
      Back
    </button>
    <button type="submit" value="Submit" class="btn btn-primary pull-right">
      Schedule
    </button>
  </div>
    </form>
    
  </div>
  
  </div>
    </div>
    <div class="span2">
    </div>
  </div>
</div>
