<% layout(layoutPath) -%>
  <div class="row">
    <div class="col-md-2" class="col-xs-1"></div>
    <div class="col-md-7">
      <div class="col-md-4">
        <h5>Something <b>Dashboard</b></h5>
      </div>
      <div class="col-md-4">
        <a href="/updateavailability">Settings</a>
      </div>
    </div>
    <div class="col-md-2"></div>
  </div>
  <div class="row">
    <div class="col-md-2" class="col-xs-1"></div>
    <div class="col-md-7">
      <h2> Settings</h2>
    </div>
    <div class="col-md-2"></div>
  </div>
  <div class="row" class="col-md-7"></row>
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-7">
        <ul class="nav nav-tabs" id="myTab">
          <li class="active"><a href="#home" data-toggle="tab"><h4>Company Settings</h4></a></li>
          <li><a href="#availability" data-toggle="tab"><h4>Availability</h4></a></li>
          <li><a href="#messages" data-toggle="tab"><h4>Prices</h4></a></li>
          <!--   <li><a href="#settings" data-toggle="tab"><h4>Settings</h4></a></li> -->
        </ul>
        <div class="tab-content">
          <div class="tab-pane active" id="home">
            <table id="basic_settings" class="table table-striped">
              <tr class="heading" bgcolor="#ccc">
              </tr>
            </table>
          </div>
          <div class="tab-pane" id="availability">
            <div class="container" id="scheduleform">
              <div class="col-md-2"></div>
              
              <div class="row">
                <div class="col-md-2"></div>
                <div class="col-md-8">
                  <div class="row">
                    <div class="col-md-12">
                      <div id="schedule-div">
                        <div name="newtimetest" id="newtimetest">
                          <table name="prestable" id="prestable" class="table table-condensed">
                            <thead>
                              <tr>
                                <th class="lastweekhead"> <i class="glyphicon glyphicon-arrow-left">
                                  </i>
                                  <a class="lastweek" href="#">
                                  last week
                                  </a>
                                </th>
                                <th name="prestable-date" id="prestable-date">asdf</th>
                                <th class="nextweekhead"> <a class="nextweek" href="#">
                                  next week
                                  </a>
                                  <i class="glyphicon glyphicon-arrow-right">
                                  </i>
                                </th>
                              </tr>
                            </thead>
                          </table>
                          <table name="stable" id="stable" class="table">
                            <thead>
                              <tr>
                                <th></th>
                                <th class="mondayhead">Mon</th>
                                <th class="tuesdayhead">Tues</th>
                                <th class="wendhead">Wend</th>
                                <th class="thurshead">Thurs</th>
                                <th class="frihead">Fri</th>
                                <th class="sathead">Sat</th>
                              </tr>
                            </thead>
                          <tbody></tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-12">
                    <div class="pull-right">
                      <!-- <button class="btn btn-default" onclick=" removebooking();">
                      <em class=
                      "icon-minus"></em>Remove</button> -->
                      <button class="btn btn-primary" id="priceNext" name="priceNext" onclick="changeStatus();"><em class="icon-edit"></em>
                      Change</button>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-md-2"></div>
            </div>
          </div>
        </div>
        <div class="tab-pane" id="messages">...</div>
        <div class="tab-pane" id="settings">...</div>
      </div>
    </div>
  </div>
