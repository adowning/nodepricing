<body>

<div id="pie"></div>

<script>
 var paper = Raphael("pie");
 var values = [18.373, 18.686, 2.867, 23.991, 9.592, 9.213];
 var legend = ["Windows/Windows Live", "Server/Tools", "Online Services", "Business", "Entertainment/Devices", "Unallocated/Other"];

 paper.piechart(
   100, // pie center x coordinate
   100, // pie center y coordinate
   90,  // pie radius
    //[18.373, 18.686, 2.867, 23.991, 9.592, 9.213] // values
    values,
       {
   legend: legend
   }
  );
</script>

<p id="copy">
	Demo of
	<a href="http://raphaeljs.com/">Raphaël</a>
	—JavaScript Vector Library
</p>
